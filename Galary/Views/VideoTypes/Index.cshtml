@model IEnumerable<Galary.Models.VideoTypes>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Video Types</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.VideoTypeName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.VideoTypeName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.VideoTypeID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.VideoTypeID })
        </td>
    </tr>
}

</table>
